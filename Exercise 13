package selfProgramming;

public class Exercise13 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// note slide 28
		// loop exercise
		for (int number =2; number <20; number++){
			number = number *2;
			if (number <15)
				System.out.println(number);
			}
	}

}

OUTPUT
4
10
