package selfProgramming;

public class Exercise8 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// to declare
		int num1, num2;
		
		for (num2 = 0; num2 <= 2; num2++) {
			for (num1 = 0; num1 <= 1; num1++) {
				System.out.println(num2 + " "+ num1);
			}
		}
	}

}

OUTPUT
0 0
0 1
1 0
1 1
2 0
2 1
