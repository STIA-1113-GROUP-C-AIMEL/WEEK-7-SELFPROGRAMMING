package selfProgramming;

public class Exercise4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// to declare
		int sum = 0;
		int number = 2;
		
		do {
			sum+=number;
			number = number + 5;
			System.out.println("SUM :" + sum);
		}
		while (number<=10);
	}

}

OUTPUT
SUM :2
SUM :9
